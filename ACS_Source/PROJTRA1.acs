function int TeamColorR(int color, int team)
{
return TeamColor(color,team,0);
}

function int TeamColorG(int color, int team)
{
return TeamColor(color,team,1);
}

function int TeamColorB(int color, int team)
{
return TeamColor(color,team,2);
}

function int TeamColor(int color, int team, int rgb)
{
int lum = palette_data_lum[color];
int lumlight = team_color_light_lum[team];
int lumdark = team_color_dark_lum[team];

if(lum>=lumlight) return GradientFunc(lum, lumlight, team_color_light[team][rgb], 255, team_color_lighter[team][rgb]);
if(lum>=lumdark) return GradientFunc(lum, lumdark, team_color_dark[team][rgb], lumlight, team_color_light[team][rgb]);
return GradientFunc(lum, 0, team_color_darker[team][rgb], lumdark, team_color_dark[team][rgb]);
return 0;
}

function int GradientFunc(int lum, int lum1, int color1, int lum2, int color2)
{
int col1Amount = lum2-lum;
int col2Amount = lum-lum1;
int colRange = col1Amount+col2Amount;

return (col1Amount*color1+col2Amount*color2)/colRange;
}