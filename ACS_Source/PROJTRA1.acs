function void InitializeColorData(void)
{
InitializeTeamColorData();
InitializePaletteDataLum();
}

int team_color_light[TEAM_COUNT][PALETTE_VAL];
int team_color_light_lum[TEAM_COUNT];
int team_color_dark[TEAM_COUNT][PALETTE_VAL];
int team_color_dark_lum[TEAM_COUNT];

int team_color_lighter[TEAM_COUNT][PALETTE_VAL];
int team_color_darker[TEAM_COUNT][PALETTE_VAL];

function void InitializeTeamColorData(void)
{
int i;

int a;
int b;
int c;
for(i = 0; i<TEAM_COUNT; i++) {
	a = team_color_input[i][0];
	b = team_color_input[i][1];
	c = team_color_input[i][2];
	
	team_color_light[i][0] = a;
	team_color_light[i][1] = b;
	team_color_light[i][2] = c;
	
	team_color_light_lum[i] = (a+b+c)/3;
	
	team_color_lighter[i][0] = (a+255)/2;
	team_color_lighter[i][1] = (b+255)/2;
	team_color_lighter[i][2] = (c+255)/2;

	a = team_color_input[i][3];
	b = team_color_input[i][4];
	c = team_color_input[i][5];
	
	team_color_dark[i][0] = a;
	team_color_dark[i][1] = b;
	team_color_dark[i][2] = c;
	
	team_color_dark_lum[i] = (a+b+c)/3;
	
	team_color_darker[i][0] = a/8;
	team_color_darker[i][1] = b/8;
	team_color_darker[i][2] = c/8;
}
}

