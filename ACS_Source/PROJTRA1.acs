int palette_data_lum[PALETTE_SIZE];

function void InitializePaletteDataLum(void)
{
int i;
int j;
int lum;
for(i = 0; i<PALETTE_SIZE; i++) {
	lum = 0;
	for(j = 0; j<PALETTE_VAL; j++) lum += palette_data[i][j];
	palette_data_lum[i] = lum/PALETTE_VAL;
}
}

